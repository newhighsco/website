{
  "name": "@newhighsco/website",
  "description": "New High Score website",
  "version": "1.15.136",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/website.git"
  },
  "homepage": "https://newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/website/issues"
  },
  "files": [
    "public",
    "src",
    ".env",
    "jsconfig.json",
    "next-sitemap.js",
    "next.config.js",
    "postcss.config.js",
    "site.config.js"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rm -rf {.next,public/robots.txt,public/sitemap.xml}",
    "start": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "serve": "next start",
    "test": "yarn lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache --ignore-path .gitignore",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.14.3",
    "@newhighsco/next-plugin-svgr": "3.0.49",
    "@newhighsco/press-start": "2.1.7",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "next": "12.1.5",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.5.1",
    "next-optimized-images": "2.6.2",
    "next-seo": "5.4.0",
    "next-sitemap": "2.5.20",
    "next-transpile-modules": "9.0.0",
    "next-videos": "1.4.0",
    "postcss": "8.4.12",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "responsive-loader": "2.3.0",
    "sass": "1.51.0",
    "sharp": "0.30.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@netlify/plugin-nextjs": "4.6.0",
    "@newhighsco/browserslist-config": "1.1.0",
    "@newhighsco/commitlint-config": "1.1.1",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "3.2.9",
    "@newhighsco/postcss-config": "3.5.10",
    "@newhighsco/prettier-config": "2.1.1",
    "@newhighsco/release-config": "1.3.4",
    "@newhighsco/stylelint-config": "3.1.4",
    "eslint": "8.14.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "stylelint": "14.8.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
