{
  "name": "@newhighsco/website",
  "description": "New High Score website",
  "version": "1.15.2",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/website.git"
  },
  "homepage": "https://newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/website/issues"
  },
  "files": [
    "public",
    "src",
    ".env",
    "jsconfig.json",
    "next-sitemap.js",
    "next.config.js",
    "postcss.config.js",
    "site.config.js"
  ],
  "scripts": {
    "clean": "rm -rf {.next,public/robots.txt,public/sitemap.xml}",
    "start": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "serve": "next start",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@newhighsco/chipset": "6.11.1",
    "@newhighsco/next-plugin-svgr": "3.0.28",
    "@newhighsco/press-start": "2.1.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "next": "11.1.2",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.5.1",
    "next-optimized-images": "2.6.2",
    "next-seo": "4.28.1",
    "next-sitemap": "1.6.179",
    "next-transpile-modules": "8.0.0",
    "next-videos": "1.4.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "responsive-loader": "2.3.0",
    "sass": "1.42.1",
    "sharp": "0.29.1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.0",
    "@newhighsco/browserslist-config": "1.0.3",
    "@newhighsco/commitlint-config": "1.0.15",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "2.3.31",
    "@newhighsco/postcss-config": "3.4.5",
    "@newhighsco/prettier-config": "2.0.14",
    "@newhighsco/release-config": "1.0.71",
    "@newhighsco/stylelint-config": "2.0.46",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "stylelint": "13.13.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
