{
  "name": "@newhighsco/website",
  "description": "New High Score website",
  "version": "1.15.121",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/website.git"
  },
  "homepage": "https://newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/website/issues"
  },
  "files": [
    "public",
    "src",
    ".env",
    "jsconfig.json",
    "next-sitemap.js",
    "next.config.js",
    "postcss.config.js",
    "site.config.js"
  ],
  "scripts": {
    "clean": "rm -rf {.next,public/robots.txt,public/sitemap.xml}",
    "start": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "serve": "next start",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache --ignore-path .gitignore",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.14.1",
    "@newhighsco/next-plugin-svgr": "3.0.48",
    "@newhighsco/press-start": "2.1.6",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "next": "12.1.4",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.5.1",
    "next-optimized-images": "2.6.2",
    "next-seo": "5.3.0",
    "next-sitemap": "2.5.17",
    "next-transpile-modules": "9.0.0",
    "next-videos": "1.4.0",
    "postcss": "8.4.12",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "responsive-loader": "2.3.0",
    "sass": "1.49.10",
    "sharp": "0.30.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@netlify/plugin-nextjs": "4.3.1",
    "@newhighsco/browserslist-config": "1.1.0",
    "@newhighsco/commitlint-config": "1.1.0",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "3.1.22",
    "@newhighsco/postcss-config": "3.5.1",
    "@newhighsco/prettier-config": "2.1.0",
    "@newhighsco/release-config": "1.3.4",
    "@newhighsco/stylelint-config": "3.0.23",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.1",
    "semantic-release": "19.0.2",
    "stylelint": "14.6.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
